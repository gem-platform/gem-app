  "dockerComposeFile": [
    "../../../docker-compose.yml", 
    "../../../docker-compose.dev.yml", 
    "docker-compose.yml"
  ],
  "service": "gem-app",
  "workspaceFolder": "/app",
  "shutdownAction": "none",
  "appPort": 9000,
